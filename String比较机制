#include<iostream>
#include<string>
using namespace std;
int main()
{
    string word = "ac";
    string name = "abc";                 //有不同字符时输出第一个不同字符
    cout<<(word<name)<<endl;
    word = "ab";
    name = "abc";                      //word是name的子集的时候word比name小
    cout<<(word<name)<<endl;                  
    return 0;
}
